{
  "disable_existing_loggers": false, 
  "formatters": {
    "detailed": {
      "format": "[%(asctime)s] %(levelname)s: %(message)s  <in %(name)s.%(funcName)s:%(lineno)d)>"
    }, 
    "fluent_fmt": {
      "()": "fluent.handler.FluentRecordFormatter", 
      "format": {
        "hostname": "%(hostname)s", 
        "level": "%(levelname)s", 
        "where": "%(module)s.%(funcName)s"
      }
    }, 
    "simple": {
      "format": "[%(asctime)s]  %(name)s::%(levelname)s - %(message)s"
    }, 
    "simple_with_proc": {
      "format": "[%(asctime)s] %(processName)s %(threadName)s %(levelname)s - %(message)s"
    }
  }, 
  "handlers": {
    "console": {
      "class": "logging.StreamHandler", 
      "formatter": "simple", 
      "level": "NOTSET", 
      "stream": "ext://sys.stdout"
    }, 
    "console_logs": {
      "class": "walkoff.appgateway.console.ConsoleLoggingHandler", 
      "formatter": "simple", 
      "level": "NOTSET"
    }, 
    "file_handler": {
      "backupCount": 20, 
      "class": "logging.handlers.RotatingFileHandler", 
      "encoding": "utf8", 
      "filename": "/home/corey/WALKOFF/data/log/log.log", 
      "formatter": "simple", 
      "level": "NOTSET", 
      "maxBytes": 10485760
    }, 
    "file_handler_with_proc": {
      "backupCount": 20, 
      "class": "logging.handlers.RotatingFileHandler", 
      "encoding": "utf8", 
      "filename": "/home/corey/WALKOFF/data/log/log.log", 
      "formatter": "simple_with_proc", 
      "level": "NOTSET", 
      "maxBytes": 10485760
    }, 
    "fluent": {
      "buffer_overflow_handler": "walkoff.config.fluent_overflow_handler", 
      "class": "fluent.handler.FluentHandler", 
      "formatter": "fluent_fmt", 
      "host": "localhost", 
      "level": "NOTSET", 
      "port": 24224, 
      "tag": "fluent.logging"
    }
  }, 
  "loggers": {
    "apps": {
      "handlers": [
        "file_handler", 
        "console_logs", 
        "fluent"
      ], 
      "level": "DEBUG"
    }, 
    "connexion": {
      "handlers": [
        "file_handler"
      ], 
      "level": "ERROR"
    }, 
    "passlib": {
      "handlers": [
        "file_handler"
      ], 
      "level": "ERROR"
    }, 
    "swagger_spec_validator": {
      "handlers": [
        "file_handler"
      ], 
      "level": "ERROR"
    }, 
    "walkoff": {
      "handlers": [
        "file_handler", 
        "fluent"
      ], 
      "level": "INFO"
    }, 
    "walkoff.appgateway.appcache": {
      "handlers": [
        "file_handler"
      ], 
      "level": "ERROR"
    }, 
    "walkoff.executiondb": {
      "handlers": [
        "file_handler_with_proc"
      ], 
      "level": "WARNING"
    }, 
    "walkoff.multiprocessedexecutor": {
      "handlers": [
        "file_handler_with_proc"
      ], 
      "level": "INFO"
    }, 
    "walkoff.server.app": {
      "handlers": [
        "file_handler"
      ], 
      "level": "ERROR"
    }, 
    "zmq": {
      "handlers": [
        "file_handler"
      ], 
      "level": "INFO"
    }
  }, 
  "root": {
    "handlers": [
      "console"
    ], 
    "level": "DEBUG"
  }, 
  "version": 1
}